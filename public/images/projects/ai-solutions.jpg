<svg width="400" height="300" viewBox="0 0 400 300" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="300" fill="#0f172a"/>
  
  <!-- 3D Print Settings Database Interface -->
  <g transform="translate(30, 30)">
    <!-- Main window -->
    <rect x="0" y="0" width="340" height="240" fill="#1e293b" stroke="#334155" stroke-width="2" rx="8"/>
    
    <!-- Header -->
    <rect x="0" y="0" width="340" height="40" fill="#7c3aed" rx="8"/>
    <rect x="0" y="32" width="340" height="8" fill="#7c3aed"/>
    <text x="170" y="25" text-anchor="middle" fill="#ffffff" font-family="Inter, sans-serif" font-size="14" font-weight="600">
      3D Print Settings Database
    </text>
    
    <!-- Printer selection -->
    <g transform="translate(20, 60)">
      <text x="0" y="15" fill="#cbd5e1" font-family="Inter, sans-serif" font-size="11" font-weight="600">
        Select Printer Model
      </text>
      
      <!-- Printer cards -->
      <rect x="0" y="20" width="140" height="60" fill="#334155" stroke="#7c3aed" stroke-width="1" rx="6"/>
      <text x="70" y="35" text-anchor="middle" fill="#7c3aed" font-family="Inter, sans-serif" font-size="10" font-weight="600">
        Creality Ender 3 S1 Pro
      </text>
      <text x="70" y="50" text-anchor="middle" fill="#94a3b8" font-family="Inter, sans-serif" font-size="8">
        FDM Printer
      </text>
      <text x="70" y="65" text-anchor="middle" fill="#10b981" font-family="Inter, sans-serif" font-size="8">
        ✓ 45 Profiles Available
      </text>
      
      <rect x="160" y="20" width="140" height="60" fill="#334155" stroke="#ff6b35" stroke-width="1" rx="6"/>
      <text x="230" y="35" text-anchor="middle" fill="#ff6b35" font-family="Inter, sans-serif" font-size="10" font-weight="600">
        Elegoo Saturn 2
      </text>
      <text x="230" y="50" text-anchor="middle" fill="#94a3b8" font-family="Inter, sans-serif" font-size="8">
        Resin Printer
      </text>
      <text x="230" y="65" text-anchor="middle" fill="#10b981" font-family="Inter, sans-serif" font-size="8">
        ✓ 28 Profiles Available
      </text>
    </g>
    
    <!-- Material settings -->
    <g transform="translate(20, 150)">
      <text x="0" y="15" fill="#cbd5e1" font-family="Inter, sans-serif" font-size="11" font-weight="600">
        Material Settings - PLA
      </text>
      
      <rect x="0" y="20" width="300" height="50" fill="#0f172a" stroke="#475569" stroke-width="1" rx="4"/>
      <text x="10" y="35" fill="#10b981" font-family="Courier New, monospace" font-size="9">
        Nozzle Temp: 210°C | Bed Temp: 60°C
      </text>
      <text x="10" y="48" fill="#cbd5e1" font-family="Courier New, monospace" font-size="9">
        Speed: 50mm/s | Layer: 0.2mm | Infill: 20%
      </text>
      <text x="10" y="61" fill="#7c3aed" font-family="Courier New, monospace" font-size="9">
        Retraction: 6mm @ 25mm/s
      </text>
    </g>
  </g>
  
  <!-- Settings icon -->
  <g transform="translate(350, 50)">
    <circle cx="20" cy="20" r="15" fill="none" stroke="#7c3aed" stroke-width="2"/>
    <circle cx="20" cy="20" r="3" fill="#7c3aed"/>
    <rect x="18" y="5" width="4" height="8" fill="#7c3aed"/>
    <rect x="18" y="27" width="4" height="8" fill="#7c3aed"/>
  </g>
  
  <!-- NosytLabs watermark -->
  <text x="350" y="290" fill="#7c3aed" font-family="Inter, sans-serif" font-size="10" opacity="0.6">
    NosytLabs
  </text>
</svg>
