<svg width="800" height="600" viewBox="0 0 800 600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e3a8a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#3b82f6;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="card" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:0.95" />
      <stop offset="100%" style="stop-color:#f8fafc;stop-opacity:0.95" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="url(#bg)"/>
  
  <!-- Header -->
  <rect x="0" y="0" width="800" height="80" fill="#1e293b" opacity="0.9"/>
  <text x="30" y="35" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="#ffffff">AI Customer Service Dashboard</text>
  <text x="30" y="55" font-family="Arial, sans-serif" font-size="12" fill="#94a3b8">Real-time Analytics & Performance Metrics</text>
  
  <!-- Navigation dots -->
  <circle cx="720" cy="25" r="6" fill="#ef4444"/>
  <circle cx="745" cy="25" r="6" fill="#f59e0b"/>
  <circle cx="770" cy="25" r="6" fill="#10b981"/>
  
  <!-- Main Content Area -->
  <rect x="20" y="100" width="760" height="480" rx="12" fill="url(#card)" stroke="#e2e8f0" stroke-width="1"/>
  
  <!-- KPI Cards -->
  <rect x="40" y="120" width="160" height="100" rx="8" fill="#ffffff" stroke="#e2e8f0"/>
  <text x="50" y="140" font-family="Arial, sans-serif" font-size="12" fill="#64748b">Response Time</text>
  <text x="50" y="165" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#0f172a">1.2s</text>
  <text x="50" y="185" font-family="Arial, sans-serif" font-size="10" fill="#10b981">↗ 15% faster</text>
  
  <rect x="220" y="120" width="160" height="100" rx="8" fill="#ffffff" stroke="#e2e8f0"/>
  <text x="230" y="140" font-family="Arial, sans-serif" font-size="12" fill="#64748b">Resolution Rate</text>
  <text x="230" y="165" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#0f172a">94.5%</text>
  <text x="230" y="185" font-family="Arial, sans-serif" font-size="10" fill="#10b981">↗ 8% improved</text>
  
  <rect x="400" y="120" width="160" height="100" rx="8" fill="#ffffff" stroke="#e2e8f0"/>
  <text x="410" y="140" font-family="Arial, sans-serif" font-size="12" fill="#64748b">Active Chats</text>
  <text x="410" y="165" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#0f172a">247</text>
  <text x="410" y="185" font-family="Arial, sans-serif" font-size="10" fill="#3b82f6">→ Real-time</text>
  
  <rect x="580" y="120" width="160" height="100" rx="8" fill="#ffffff" stroke="#e2e8f0"/>
  <text x="590" y="140" font-family="Arial, sans-serif" font-size="12" fill="#64748b">Satisfaction</text>
  <text x="590" y="165" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#0f172a">4.8/5</text>
  <text x="590" y="185" font-family="Arial, sans-serif" font-size="10" fill="#10b981">↗ 0.3 points</text>
  
  <!-- Chart Area -->
  <rect x="40" y="240" width="360" height="200" rx="8" fill="#ffffff" stroke="#e2e8f0"/>
  <text x="50" y="260" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#0f172a">Performance Trends</text>
  
  <!-- Simple line chart -->
  <polyline points="60,400 100,380 140,360 180,340 220,320 260,310 300,300 340,290 380,280" 
            fill="none" stroke="#3b82f6" stroke-width="3" stroke-linecap="round"/>
  <polyline points="60,420 100,410 140,400 180,390 220,385 260,380 300,375 340,370 380,365" 
            fill="none" stroke="#10b981" stroke-width="2" stroke-linecap="round"/>
  
  <!-- Chart grid lines -->
  <line x1="60" y1="280" x2="380" y2="280" stroke="#f1f5f9" stroke-width="1"/>
  <line x1="60" y1="320" x2="380" y2="320" stroke="#f1f5f9" stroke-width="1"/>
  <line x1="60" y1="360" x2="380" y2="360" stroke="#f1f5f9" stroke-width="1"/>
  <line x1="60" y1="400" x2="380" y2="400" stroke="#f1f5f9" stroke-width="1"/>
  
  <!-- Activity Feed -->
  <rect x="420" y="240" width="320" height="200" rx="8" fill="#ffffff" stroke="#e2e8f0"/>
  <text x="430" y="260" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#0f172a">Recent Activity</text>
  
  <!-- Activity items -->
  <circle cx="440" cy="285" r="4" fill="#10b981"/>
  <text x="455" y="290" font-family="Arial, sans-serif" font-size="11" fill="#0f172a">Chat resolved: Order #12345</text>
  <text x="455" y="302" font-family="Arial, sans-serif" font-size="9" fill="#64748b">2 minutes ago</text>
  
  <circle cx="440" cy="320" r="4" fill="#3b82f6"/>
  <text x="455" y="325" font-family="Arial, sans-serif" font-size="11" fill="#0f172a">New chat initiated</text>
  <text x="455" y="337" font-family="Arial, sans-serif" font-size="9" fill="#64748b">5 minutes ago</text>
  
  <circle cx="440" cy="355" r="4" fill="#f59e0b"/>
  <text x="455" y="360" font-family="Arial, sans-serif" font-size="11" fill="#0f172a">Escalation: Technical issue</text>
  <text x="455" y="372" font-family="Arial, sans-serif" font-size="9" fill="#64748b">8 minutes ago</text>
  
  <circle cx="440" cy="390" r="4" fill="#10b981"/>
  <text x="455" y="395" font-family="Arial, sans-serif" font-size="11" fill="#0f172a">Chat resolved: Billing inquiry</text>
  <text x="455" y="407" font-family="Arial, sans-serif" font-size="9" fill="#64748b">12 minutes ago</text>
  
  <!-- AI Status Indicator -->
  <rect x="40" y="460" width="700" height="40" rx="8" fill="#f0fdf4" stroke="#bbf7d0"/>
  <circle cx="60" cy="480" r="6" fill="#10b981"/>
  <text x="80" y="485" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#166534">AI System Status: Operational</text>
  <text x="300" y="485" font-family="Arial, sans-serif" font-size="11" fill="#166534">Processing 247 concurrent conversations</text>
  
  <!-- Footer -->
  <rect x="0" y="580" width="800" height="20" fill="#1e293b" opacity="0.9"/>
  <text x="30" y="595" font-family="Arial, sans-serif" font-size="10" fill="#94a3b8">NosytLabs AI Integration Platform - Live Dashboard</text>
</svg>