<svg width="400" height="300" viewBox="0 0 400 300" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="300" fill="#0f172a"/>
  
  <!-- Content Creation Resources Interface -->
  <g transform="translate(30, 30)">
    <!-- Main window -->
    <rect x="0" y="0" width="340" height="240" fill="#1e293b" stroke="#334155" stroke-width="2" rx="8"/>
    
    <!-- Header -->
    <rect x="0" y="0" width="340" height="40" fill="#ff6b35" rx="8"/>
    <rect x="0" y="32" width="340" height="8" fill="#ff6b35"/>
    <text x="170" y="25" text-anchor="middle" fill="#ffffff" font-family="Inter, sans-serif" font-size="14" font-weight="600">
      Content Creation Resources
    </text>
    
    <!-- Resource cards -->
    <g transform="translate(20, 60)">
      <!-- Streaming Setup Guide -->
      <rect x="0" y="0" width="140" height="80" fill="#334155" stroke="#7c3aed" stroke-width="1" rx="6"/>
      <text x="70" y="20" text-anchor="middle" fill="#7c3aed" font-family="Inter, sans-serif" font-size="11" font-weight="600">
        Streaming Setup
      </text>
      <text x="70" y="35" text-anchor="middle" fill="#94a3b8" font-family="Inter, sans-serif" font-size="8">
        Complete guide for
      </text>
      <text x="70" y="48" text-anchor="middle" fill="#94a3b8" font-family="Inter, sans-serif" font-size="8">
        YouTube &amp; Kick.com
      </text>
      <rect x="10" y="55" width="120" height="18" fill="#7c3aed" rx="3"/>
      <text x="70" y="66" text-anchor="middle" fill="#ffffff" font-family="Inter, sans-serif" font-size="8">View Guide</text>
      
      <!-- Thumbnail Templates -->
      <rect x="160" y="0" width="140" height="80" fill="#334155" stroke="#10b981" stroke-width="1" rx="6"/>
      <text x="230" y="20" text-anchor="middle" fill="#10b981" font-family="Inter, sans-serif" font-size="11" font-weight="600">
        Thumbnail Templates
      </text>
      <text x="230" y="35" text-anchor="middle" fill="#94a3b8" font-family="Inter, sans-serif" font-size="8">
        Professional designs
      </text>
      <text x="230" y="48" text-anchor="middle" fill="#94a3b8" font-family="Inter, sans-serif" font-size="8">
        for content creators
      </text>
      <rect x="170" y="55" width="120" height="18" fill="#10b981" rx="3"/>
      <text x="230" y="66" text-anchor="middle" fill="#ffffff" font-family="Inter, sans-serif" font-size="8">Download</text>
    </g>
    
    <!-- Analytics section -->
    <g transform="translate(20, 160)">
      <text x="0" y="15" fill="#cbd5e1" font-family="Inter, sans-serif" font-size="11" font-weight="600">
        Basic Analytics Tracking
      </text>
      
      <rect x="0" y="20" width="300" height="50" fill="#0f172a" stroke="#475569" stroke-width="1" rx="4"/>
      <text x="10" y="35" fill="#ff6b35" font-family="Courier New, monospace" font-size="9">
        // Track viewer engagement
      </text>
      <text x="10" y="48" fill="#10b981" font-family="Courier New, monospace" font-size="9">
        analytics.track('video_view', { duration: 120 });
      </text>
      <text x="10" y="61" fill="#7c3aed" font-family="Courier New, monospace" font-size="9">
        console.log('Engagement tracked!');
      </text>
    </g>
  </g>
  
  <!-- Content creation icons -->
  <g transform="translate(350, 50)">
    <!-- Camera icon -->
    <rect x="0" y="0" width="30" height="20" fill="none" stroke="#ff6b35" stroke-width="2" rx="4"/>
    <circle cx="15" cy="10" r="6" fill="none" stroke="#ff6b35" stroke-width="2"/>
    <circle cx="15" cy="10" r="3" fill="#ff6b35"/>
  </g>
  
  <!-- NosytLabs watermark -->
  <text x="350" y="290" fill="#7c3aed" font-family="Inter, sans-serif" font-size="10" opacity="0.6">
    NosytLabs
  </text>
</svg>
