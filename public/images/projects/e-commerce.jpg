<svg width="400" height="300" viewBox="0 0 400 300" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="300" fill="#f8fafc"/>
  
  <!-- Browser window -->
  <g transform="translate(20, 20)">
    <!-- Window frame -->
    <rect x="0" y="0" width="360" height="260" fill="#ffffff" stroke="#e2e8f0" stroke-width="2" rx="8"/>
    
    <!-- Browser header -->
    <rect x="0" y="0" width="360" height="40" fill="#f1f5f9" rx="8"/>
    <rect x="0" y="32" width="360" height="8" fill="#f1f5f9"/>
    
    <!-- Browser buttons -->
    <circle cx="20" cy="20" r="6" fill="#ef4444"/>
    <circle cx="40" cy="20" r="6" fill="#f59e0b"/>
    <circle cx="60" cy="20" r="6" fill="#10b981"/>
    
    <!-- URL bar -->
    <rect x="90" y="12" width="200" height="16" fill="#ffffff" stroke="#d1d5db" stroke-width="1" rx="8"/>
    <text x="95" y="22" fill="#6b7280" font-family="Inter, sans-serif" font-size="8">
      nosytlabs.com/tools
    </text>
    
    <!-- Page content -->
    <g transform="translate(20, 60)">
      <!-- Header -->
      <text x="0" y="20" fill="#1f2937" font-family="Inter, sans-serif" font-size="16" font-weight="600">
        Development Utilities
      </text>
      
      <!-- Tool cards -->
      <g transform="translate(0, 40)">
        <!-- JSON Validator -->
        <rect x="0" y="0" width="100" height="60" fill="#7c3aed" rx="8"/>
        <text x="50" y="25" text-anchor="middle" fill="#ffffff" font-family="Inter, sans-serif" font-size="10" font-weight="600">
          JSON
        </text>
        <text x="50" y="35" text-anchor="middle" fill="#ffffff" font-family="Inter, sans-serif" font-size="10" font-weight="600">
          Validator
        </text>
        <text x="50" y="50" text-anchor="middle" fill="#c4b5fd" font-family="Inter, sans-serif" font-size="8">
          Format &amp; Validate
        </text>
        
        <!-- Base64 Encoder -->
        <rect x="110" y="0" width="100" height="60" fill="#ff6b35" rx="8"/>
        <text x="160" y="25" text-anchor="middle" fill="#ffffff" font-family="Inter, sans-serif" font-size="10" font-weight="600">
          Base64
        </text>
        <text x="160" y="35" text-anchor="middle" fill="#ffffff" font-family="Inter, sans-serif" font-size="10" font-weight="600">
          Encoder
        </text>
        <text x="160" y="50" text-anchor="middle" fill="#fed7d7" font-family="Inter, sans-serif" font-size="8">
          Encode &amp; Decode
        </text>
        
        <!-- Color Palette -->
        <rect x="220" y="0" width="100" height="60" fill="#10b981" rx="8"/>
        <text x="270" y="25" text-anchor="middle" fill="#ffffff" font-family="Inter, sans-serif" font-size="10" font-weight="600">
          Color
        </text>
        <text x="270" y="35" text-anchor="middle" fill="#ffffff" font-family="Inter, sans-serif" font-size="10" font-weight="600">
          Palette
        </text>
        <text x="270" y="50" text-anchor="middle" fill="#d1fae5" font-family="Inter, sans-serif" font-size="8">
          Generate Colors
        </text>
      </g>
      
      <!-- Code formatter section -->
      <g transform="translate(0, 120)">
        <rect x="0" y="0" width="320" height="60" fill="#1f2937" stroke="#374151" stroke-width="1" rx="8"/>
        <text x="10" y="20" fill="#10b981" font-family="Courier New, monospace" font-size="8">
          // Code Formatter
        </text>
        <text x="10" y="35" fill="#cbd5e1" font-family="Courier New, monospace" font-size="8">
          const formatted = beautify(code);
        </text>
        <text x="10" y="50" fill="#7c3aed" font-family="Courier New, monospace" font-size="8">
          console.log('Formatted!');
        </text>
      </g>
    </g>
  </g>
  
  <!-- NosytLabs watermark -->
  <text x="350" y="290" fill="#7c3aed" font-family="Inter, sans-serif" font-size="10" opacity="0.6">
    NosytLabs
  </text>
</svg>
