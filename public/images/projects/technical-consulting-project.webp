  <rect x="650" y="240" width="40" height="25" rx="2" fill="#065f46" stroke="#10b981"/>
  <text x="665" y="255" font-family="Arial, sans-serif" font-size="8" fill="#ffffff">Cache</text>
  
  <rect x="700" y="240" width="40" height="25" rx="2" fill="#065f46" stroke="#10b981"/>
  <text x="715" y="255" font-family="Arial, sans-serif" font-size="8" fill="#ffffff">Queue</text>
  
