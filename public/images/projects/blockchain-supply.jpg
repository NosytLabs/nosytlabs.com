<svg width="400" height="300" viewBox="0 0 400 300" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="300" fill="#1a1a2e"/>
  
  <!-- 3D Printer interface -->
  <g transform="translate(50, 50)">
    <!-- Calculator window -->
    <rect x="0" y="0" width="300" height="200" fill="#2d3748" stroke="#4a5568" stroke-width="2" rx="8"/>
    
    <!-- Header -->
    <rect x="0" y="0" width="300" height="30" fill="#7c3aed" rx="8"/>
    <rect x="0" y="22" width="300" height="8" fill="#7c3aed"/>
    <text x="150" y="20" text-anchor="middle" fill="#ffffff" font-family="Inter, sans-serif" font-size="12" font-weight="600">
      3D Print Cost Calculator
    </text>
    
    <!-- Input section -->
    <g transform="translate(20, 50)">
      <text x="0" y="15" fill="#cbd5e1" font-family="Inter, sans-serif" font-size="10">Print Settings</text>
      
      <!-- Material dropdown -->
      <rect x="0" y="20" width="120" height="20" fill="#1a202c" stroke="#7c3aed" stroke-width="1" rx="4"/>
      <text x="5" y="32" fill="#cbd5e1" font-family="Inter, sans-serif" font-size="9">PLA - $25/kg</text>
      
      <!-- Print time -->
      <rect x="130" y="20" width="120" height="20" fill="#1a202c" stroke="#7c3aed" stroke-width="1" rx="4"/>
      <text x="135" y="32" fill="#cbd5e1" font-family="Inter, sans-serif" font-size="9">Time: 4h 30m</text>
      
      <!-- Weight -->
      <rect x="0" y="50" width="120" height="20" fill="#1a202c" stroke="#7c3aed" stroke-width="1" rx="4"/>
      <text x="5" y="62" fill="#cbd5e1" font-family="Inter, sans-serif" font-size="9">Weight: 45g</text>
      
      <!-- Power consumption -->
      <rect x="130" y="50" width="120" height="20" fill="#1a202c" stroke="#7c3aed" stroke-width="1" rx="4"/>
      <text x="135" y="62" fill="#cbd5e1" font-family="Inter, sans-serif" font-size="9">Power: 200W</text>
    </g>
    
    <!-- Results section -->
    <g transform="translate(20, 130)">
      <rect x="0" y="0" width="260" height="50" fill="#0f3460" stroke="#ff6b35" stroke-width="1" rx="4"/>
      <text x="10" y="15" fill="#ff6b35" font-family="Inter, sans-serif" font-size="12" font-weight="600">
        Total Cost Breakdown
      </text>
      <text x="10" y="30" fill="#10b981" font-family="Inter, sans-serif" font-size="10">
        Material: $1.13 | Electricity: $0.24 | Total: $1.37
      </text>
      <text x="10" y="42" fill="#cbd5e1" font-family="Inter, sans-serif" font-size="9">
        Recommended Price: $4.11 (3x markup)
      </text>
    </g>
  </g>
  
  <!-- 3D printer illustration -->
  <g transform="translate(320, 80)">
    <!-- Printer base -->
    <rect x="0" y="40" width="60" height="20" fill="#4a5568" rx="4"/>
    <!-- Printer frame -->
    <rect x="10" y="20" width="40" height="40" fill="none" stroke="#cbd5e1" stroke-width="2"/>
    <!-- Print bed -->
    <rect x="15" y="35" width="30" height="2" fill="#7c3aed"/>
    <!-- Extruder -->
    <rect x="25" y="25" width="10" height="8" fill="#ff6b35"/>
  </g>
  
  <!-- NosytLabs watermark -->
  <text x="350" y="290" fill="#7c3aed" font-family="Inter, sans-serif" font-size="10" opacity="0.6">
    NosytLabs
  </text>
</svg>
