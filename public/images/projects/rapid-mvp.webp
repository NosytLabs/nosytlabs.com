<svg width="400" height="300" viewBox="0 0 400 300" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="300" fill="#f8fafc"/>
  
  <!-- Dashboard Header -->
  <rect x="20" y="20" width="360" height="50" rx="8" fill="url(#mvpGradient)"/>
  <text x="40" y="50" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="white">MVP Dashboard</text>
  
  <!-- Status Indicator -->
  <circle cx="320" cy="45" r="8" fill="#10b981"/>
  <text x="340" y="50" font-family="Arial, sans-serif" font-size="10" fill="white">Live</text>
  
  <!-- Main Content Area -->
  <rect x="20" y="90" width="180" height="180" rx="8" fill="white" stroke="#e2e8f0"/>
  <text x="30" y="110" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#1e293b">Quick Prototype</text>
  
  <!-- Feature Cards -->
  <rect x="30" y="120" width="160" height="40" rx="4" fill="#f1f5f9"/>
  <text x="40" y="140" font-family="Arial, sans-serif" font-size="10" fill="#64748b">✓ User Authentication</text>
  
  <rect x="30" y="170" width="160" height="40" rx="4" fill="#f1f5f9"/>
  <text x="40" y="190" font-family="Arial, sans-serif" font-size="10" fill="#64748b">✓ Basic CRUD Operations</text>
  
  <rect x="30" y="220" width="160" height="40" rx="4" fill="#f1f5f9"/>
  <text x="40" y="240" font-family="Arial, sans-serif" font-size="10" fill="#64748b">✓ API Integration</text>
  
  <!-- Sidebar -->
  <rect x="220" y="90" width="160" height="180" rx="8" fill="white" stroke="#e2e8f0"/>
  <text x="230" y="110" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#1e293b">Development Stats</text>
  
  <!-- Stats -->
  <text x="230" y="135" font-family="Arial, sans-serif" font-size="12" fill="#64748b">Time to MVP:</text>
  <text x="230" y="150" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#7c3aed">5 Days</text>
  
  <text x="230" y="175" font-family="Arial, sans-serif" font-size="12" fill="#64748b">Features:</text>
  <text x="230" y="190" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#ff6b35">12</text>
  
  <text x="230" y="215" font-family="Arial, sans-serif" font-size="12" fill="#64748b">Status:</text>
  <text x="230" y="230" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#10b981">Ready</text>
  
  <!-- Gradient Definition -->
  <defs>
    <linearGradient id="mvpGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#7c3aed;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ff6b35;stop-opacity:1" />
    </linearGradient>
  </defs>
</svg>
