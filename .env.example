# NosytLabs Environment Variables
# Copy this file to .env.local and fill in your values

# YouTube API Configuration
YOUTUBE_API_KEY=your_youtube_api_key_here
YOUTUBE_CHANNEL_ID=your_channel_id_here

# Kick.com Integration
KICK_USERNAME=your_kick_username
KICK_API_ENDPOINT=https://kick.com/api/v2

# Creality Cloud Integration
CREALITY_API_KEY=your_creality_api_key
CREALITY_USER_ID=your_creality_user_id

# Admin Panel Configuration
ADMIN_SECRET_KEY=your_secret_key_for_admin_panel
SESSION_SECRET=your_session_secret_key

# Email Configuration (for contact forms)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password

# Analytics
VERCEL_ANALYTICS_ID=your_vercel_analytics_id
GOOGLE_ANALYTICS_ID=your_google_analytics_id

# Development
NODE_ENV=development
