{
  "name": "nosytlabs-website",
  "version": "1.0.0",
  "description": "NosytLabs Portfolio Website",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "dev:debug": "astro dev --verbose",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "eslint .",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist .astro node_modules/.cache",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "browser-sync": "browser-sync start --config bs-config.js",
    "dev:multi-browser": "concurrently \"npm run dev\" \"npm run browser-sync\"",
    "test:browsers": "npm run build && npm run test:cross-browser",
    "test:cross-browser": "node scripts/cross-browser-test.js",
    "wait-for-server": "wait-on http://localhost:8080",
    "check:prefixes": "autoprefixer --info",
    "validate:css": "stylelint 'src/**/*.css' --config .stylelintrc.json"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/node": "^9.3.0",
    "@neondatabase/serverless": "^1.0.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "astro": "^5.8.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.23.0",
    "glob": "^11.0.0",
    "lightningcss": "^1.30.1",
    "lucide-react": "^0.514.0",
    "puppeteer": "^24.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resend": "^4.6.0",
    "tailwind-merge": "^3.3.1",
    "wait-on": "^8.0.3"
  },
  "devDependencies": {
    "@astrojs/react": "^4.3.0",
    "@astrojs/tailwind": "^5.1.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/aria-query": "^5.0.4",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.20.7",
    "@types/chai": "^5.2.2",
    "@types/connect": "^3.4.38",
    "@types/debug": "^4.1.12",
    "@types/estree": "^1.0.8",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/ms": "^2.1.0",
    "@types/mysql": "^2.15.27",
    "@types/nlcst": "^2.0.3",
    "@types/node": "^22.16.0",
    "@types/pg": "^8.15.4",
    "@types/pg-pool": "^2.0.6",
    "@types/phoenix": "^1.6.6",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/shimmer": "^1.2.0",
    "@types/tedious": "^4.0.14",
    "@types/unist": "^3.0.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "astro-eslint-parser": "^1.2.2",
    "autoprefixer": "^10.4.17",
    "browser-sync": "^3.0.4",
    "concurrently": "^9.2.0",
    "core-js": "^3.23.5",
    "esbuild": "^0.25.5",
    "eslint": "^9.30.1",
    "jsdom": "^23.0.1",
    "playwright": "^1.54.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.5.3",
    "stylelint": "^16.21.1",
    "stylelint-config-standard": "^38.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3"
  },
  "overrides": {
    "path-to-regexp": "^6.3.0"
  },
  "keywords": [
    "astro",
    "portfolio",
    "nosytlabs"
  ],
  "author": "NosytLabs",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  }
