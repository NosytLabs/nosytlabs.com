{
  "name": "nosytlabs-github-ready",
  "version": "1.0.0",
  "description": "A modern Astro-based website for NosytLabs with optimized performance and accessibility",
  "type": "module",
  "scripts": {
    "dev": "astro dev --port 5000 --host 0.0.0.0",
    "start": "astro dev --port 5000 --host 0.0.0.0",
    "build": "astro check && astro build",
    "preview": "astro preview --port 5000 --host 0.0.0.0",
    "astro": "astro",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.astro --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "astro check && tsc --noEmit"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.28.1"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.7",
    "@astrojs/react": "^3.6.2",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/tailwind": "^5.1.2",
    "@emailjs/browser": "^4.4.1",
    "@heroui/react": "^2.8.5",
    "@nosytlabs/shared-utils": "file:packages/shared-utils",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-toast": "^1.2.15",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "astro": "^5.14.7",
    "astro-icon": "^1.1.5",
    "class-variance-authority": "^0.7.1",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.545.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.14"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "@typescript-eslint/typescript-estree": "^8.46.2",
    "astro-compressor": "^1.1.2",
    "eslint": "^9.14.0",
    "eslint-plugin-astro": "^1.2.4",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "fast-glob": "^3.3.3",
    "glob": "^11.0.3",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "astro",
    "website",
    "nosytlabs",
    "typescript",
    "tailwindcss",
    "performance",
    "accessibility",
    "seo"
  ],
  "author": "NosytLabs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nosytlabs/nosytlabs-github-ready.git"
  },
  "bugs": {
    "url": "https://github.com/nosytlabs/nosytlabs-github-ready/issues"
  },
  "homepage": "https://github.com/nosytlabs/nosytlabs-github-ready#readme"
}
