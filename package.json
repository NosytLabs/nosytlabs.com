{
  "name": "nosytlabs-website",
  "version": "1.0.0",
  "description": "NosytLabs Portfolio Website",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "npm run fix-css && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "npx playwright test",
    "test:debug": "npx playwright test --debug",
    "optimize-images": "node scripts/optimize-images.js",
    "generate-win95-icons": "node scripts/build/generate-win95-icons.js",
    "generate-resize-handle": "node scripts/build/generate-resize-handle.js",
    "generate-favicons": "node scripts/build/generate-favicons.js",
    "generate-assets": "node scripts/build/generate-assets.js",
    "check-links": "node scripts/development/check-links.js",
    "fix-css": "node scripts/development/fix-css.js",
    "build:prod": "npm run generate-assets && npm run fix-css && astro build",
    "build:full": "npm run generate-assets && npm run fix-css && astro build && npm run check-links",
    "optimize:images": "node scripts/optimization/image-optimization.js",
    "optimize:css": "node scripts/optimization/css-optimization.js",
    "test:performance": "node scripts/optimization/performance-test.js",
    "optimize:all": "npm run optimize:images && npm run optimize:css",
    "consolidate:scripts": "node scripts/optimization/consolidate-scripts.js",
    "monitor:performance": "node scripts/optimization/performance-monitor.js",
    "build:optimized": "npm run optimize:all && npm run consolidate:scripts && npm run build",
    "lighthouse": "npx lighthouse http://localhost:4321 --output html --output-path ./performance-reports/lighthouse.html",
    "perf:audit": "npm run build && npm run preview & sleep 5 && npm run test:performance && kill %1"
  },
  "dependencies": {
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "astro": "^4.5.0",
    "puppeteer": "^24.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@astrojs/react": "^3.1.0",
    "@astrojs/tailwind": "^5.1.0",
    "@playwright/test": "^1.52.0",
    "@types/node": "^22.15.3",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "lightningcss": "^1.30.0",
    "serve": "^14.2.4",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.1",
    "terser": "^5.39.0"
  },
  "keywords": [
    "astro",
    "portfolio",
    "nosytlabs"
  ],
  "author": "NosytLabs",
  "license": "ISC"
}
