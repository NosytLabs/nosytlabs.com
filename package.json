    "serve": "^14.2.4",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.1",
    "terser": "^5.39.0",
    "postcss": "^8.4.35",
    "autoprefixer": "^10.4.17",
    "cssnano": "^6.0.3"
