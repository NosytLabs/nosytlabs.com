    "isomorphic-dompurify": "^2.26.0",
    "jose": "^6.0.12",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.526.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.1.0",
    "zod": "^3.25.76"
  },
  "overrides": {
    "undici": "^5.29.0",
    "esbuild": "^0.25.9"
  },
  "keywords": [
    "astro",
    "projects",
    "nosytlabs"
