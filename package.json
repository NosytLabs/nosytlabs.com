{
  "name": "nosytlabs-website",
  "version": "1.0.0",
  "description": "NosytLabs Professional Web Development & AI Integration Services",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "start": "astro dev",
    "start:e2e": "astro dev --port 4321 --host",
    "build": "astro build",
    "preview": "astro preview",
    "build:vercel": "astro build && npm run optimize:build",
    "optimize:build": "echo 'Build optimized for Vercel'",
    "lint": "npx eslint . --quiet",
    "lint:fix": "npx eslint . --fix",
    "format": "npx prettier --write .",
    "type-check": "npx tsc --noEmit",
    "clean": "npx rimraf dist .astro node_modules/.cache",
    "test": "npx vitest run",
    "test:coverage": "npx vitest run --coverage",
    "analyze": "npx vite-bundle-analyzer dist/client",
    "build:analyze": "npm run build && npm run analyze",
    "perf:budget": "node scripts/validate-performance-budget.js",
    "perf:budget:ci": "node scripts/validate-performance-budget.js performance-budget.json performance-report.json",
    "lighthouse": "npx lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "audit:perf": "npm run build && npx @lhci/cli@0.13.x autorun --collect.staticDistDir=dist --collect.settings.onlyCategories=performance --collect.numberOfRuns=1 --upload.target=filesystem --upload.outputDir=.reports --assert.preset=lighthouse:no-pwa",
    "security:scan": "node scripts/security-scan.js",
    "security:audit": "npm audit --audit-level=moderate",
    "security:update": "npm update && npm audit fix",
    "audit:contrast": "node scripts/color-contrast-audit.js",
    "audit:accessibility": "npm run audit:contrast",
    "lint:css": "stylelint \"src/styles/**/*.css\"",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "client:dev": "vite",
    "server:dev": "npx nodemon",
    "preview:integrated": "npm run build && cross-env NODE_ENV=production SERVE_STATIC=true PORT=4321 tsx api/server.ts",
    "purge:css": "node purgecss.config.js",
    "generate:tokens": "ts-node --esm scripts/generate-css-tokens.ts",
    "tokens:watch": "nodemon --watch src/styles/tokens/index.ts --exec \"npm run generate:tokens\""
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^3.6.2",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/tailwind": "^5.1.2",
    "@astrojs/vercel": "^8.0.4",
    "@emailjs/browser": "^4.4.1",
    "@hookform/resolvers": "^5.2.1",
