{
  "name": "nosytlabs-website",
  "version": "1.0.0",
  "description": "NosytLabs Portfolio Website",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "npm run optimize:css && astro build && npm run analyze:build",
    "build:production": "npm run clean && npm run optimize:css && astro build && npm run analyze:build",
    "preview": "astro preview",
    "lint": "eslint .",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist .astro node_modules/.cache",
    "optimize:css": "node scripts/css-optimizer.js",
    "analyze:css": "node scripts/css-optimizer.js --analyze-only",
    "analyze:build": "node scripts/build-analyzer.js",
    "analyze:performance": "node scripts/build-analyzer.js --performance-only",
    "lint:fix": "eslint src/ --fix",
    "quality:check": "npm run lint:fix && npm run type-check",
    "optimize:performance": "npm run optimize:css && npm run analyze:build",
    "test:performance": "npm run build && npm run analyze:performance",
    "optimize:all": "npm run quality:check && npm run optimize:performance",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:wallaby": "vitest run --reporter=json --outputFile=coverage/wallaby-test-results.json"
  },
  "dependencies": {
    "@astrojs/vercel": "^8.0.4",
    "@neondatabase/serverless": "^1.0.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "astro": "^5.8.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.17.3",
    "lucide-react": "^0.514.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@astrojs/react": "^4.3.0",
    "@astrojs/tailwind": "^5.1.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^22.15.3",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "astro-eslint-parser": "^1.2.2",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.1",
    "jsdom": "^23.0.1",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.6.3",
    "vitest": "^3.2.3"
  },
  "overrides": {
    "path-to-regexp": "^6.3.0"
  },
  "keywords": [
    "astro",
    "portfolio",
    "nosytlabs"
  ],
  "author": "NosytLabs",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  }
}
